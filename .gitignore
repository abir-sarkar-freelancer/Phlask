# Ignore system files
.DS_Store
Thumbs.db

# Composer generated files
/composer.lock

# Ignore any logs, cache, and temp files
/logs/*
/cache/*
/temp/*

# Ignore environment files
.env
.env.local
.env.*.local

# Ignore IDE config files
.vscode/
.idea/

# Ignore PHPUnit result cache
.phpunit.result.cache

# Ignore coverage reports
coverage/

# Ignore compiled classes
/vendor/.composer
/vendor/bin/

# Ignore node_modules (if any)
/node_modules

# Ignore static assets that are generated
/public/static/*.map
/public/static/*.css.map
/public/static/*.js.map

# Ignore SASS cache
.sass-cache/

# Ignore npm debug log
npm-debug.log
yarn-debug.log
yarn-error.log
